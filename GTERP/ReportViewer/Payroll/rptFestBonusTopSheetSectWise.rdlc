Dim TM As String ' If MyNumber between Rs.1 To 99 Taka Only.
TM = Right(MyNumber, 2)

If Len(MyNumber) &gt; 0 And Len(MyNumber) &lt;= 2 Then
If Len(TM) = 1 Then
Words = ConvertDigit(TM)
RupeesToWord = " " &amp; Words &amp; Paisa &amp; " Taka Only"

Exit Function

Else
If Len(TM) = 2 Then
Words = ConvertTens(TM)
RupeesToWord = " " &amp; Words &amp; Paisa &amp; " Taka Only"
Exit Function

End If
End If
End If
'===============================================================


' Convert last 3 digits of MyNumber to ruppees in word.
Hundred = ConvertHundreds(Right(MyNumber, 3))
' Strip off last three digits
MyNumber = Left(MyNumber, Len(MyNumber) - 3)

iCount = 0
Do While MyNumber &lt;&gt; ""
'Strip last two digits
Temp = Right(MyNumber, 2)
If Len(MyNumber) = 1 Then


If Trim(Words) = "Thousand" Or _
Trim(Words) = "Lac Thousand" Or _
Trim(Words) = "Lac" Or _
Trim(Words) = "Crore" Or _
Trim(Words) = "Crore Lac Thousand" Or _
Trim(Words) = "Arab Crore Lac Thousand" Or _
Trim(Words) = "Arab" Or _
Trim(Words) = "Kharab Arab Crore Lac Thousand" Or _
Trim(Words) = "Kharab" Then

