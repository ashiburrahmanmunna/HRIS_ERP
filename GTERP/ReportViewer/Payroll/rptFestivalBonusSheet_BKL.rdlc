  <Code>Public Function GTRFormatCurrencyBDT(num As String, floatingPointSize As Int16, IsUseBracket As Boolean) As String
        Dim returnValue As String = "", strNegSymbol As String = ""
        Try
            If (num.Substring(0, 1).ToString() = "-") Then strNegSymbol = "-"

            num = num.Replace(",", "")

            Dim strFPS As String = "n" &amp; String.Format("{0}", floatingPointSize.ToString())
            num = String.Format("{0:" &amp; strFPS &amp; "}", Math.Abs(Double.Parse(num)))
            Dim GeneralValue As String = "0", floatValue As String = "0"
            Dim i As Integer = num.IndexOf(".")

            GeneralValue = num

            If i &gt; 0 Then
                GeneralValue = num.Substring(0, num.IndexOf(".")).Replace(",", "")
                floatValue = num.Substring(num.IndexOf(".") + 1, num.Length - (num.IndexOf(".") + 1))
            End If

            Dim gv As String = ""
            Dim c As Char() = GeneralValue.ToCharArray()
            Array.Reverse(c)
            GeneralValue = New String(c)

            Dim startDed As Integer = 0, dedChar As Integer = 3
            For m As Integer = 0 To GeneralValue.Length - 1
                If startDed + dedChar &gt;= GeneralValue.Length Then
                    gv = gv &amp; GeneralValue.Substring(startDed, GeneralValue.Length - startDed) &amp; ","
                    Exit For
                Else
                    gv = gv &amp; GeneralValue.Substring(startDed, dedChar) &amp; ","
                End If

                startDed = startDed + dedChar
                dedChar = 2
            Next

            GeneralValue = gv.Substring(0, gv.Length - 1)
            c = GeneralValue.ToCharArray()
            Array.Reverse(c)
            GeneralValue = New String(c)

            returnValue = strNegSymbol &amp; GeneralValue &amp; "." &amp; floatValue
            If (strNegSymbol.Length &gt; 0) And (IsUseBracket = True) Then
                returnValue = "(" &amp; GeneralValue &amp; "." &amp; floatValue &amp; ")"
            End If
            ''returnValue = GeneralValue &amp; "." &amp; floatValue
        Catch ex As Exception
            Throw (ex)
        End Try
        
        Return returnValue
    End Function</Code>
