        function AmtCalc() {

            var convrate = $('#ConvRateSelect').val() || 1;
            var DebitAmount = $('#DebitAmount').val() || 0;
            var CreditAmount = $('#CreditAmount').val() || 0;


            if ($('#DebitAmount').val().length == 0) {
                $('#DebitAmount').val(0);

            }
            if ($('#CreditAmount').val().length == 0) {
                $('#CreditAmount').val(0);

            }


            if (convrate.length == 0) {
                $('#ConvRateSelect').val(1);

            }
            else if (convrate < 1) {
                $('#ConvRateSelect').val(1);
            }

            convrate = $('#ConvRateSelect').val();
            if (DebitAmount > 0 || CreditAmount > 0) {

                var x = DebitAmount * convrate;
                var y = CreditAmount * convrate;


                $('#TKDebitLocal').val(x.toFixed(4));
                $('#TKCreditLocal').val(y.toFixed(4));

            }
        };

