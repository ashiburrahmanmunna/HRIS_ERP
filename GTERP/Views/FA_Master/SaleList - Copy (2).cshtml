@model IEnumerable<GTERP.Models.FA_Sell>


    <table id="tblSold_Asset_List" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
        <thead>
            <tr>

                <th>
                    @Html.DisplayNameFor(model => model.FA_Details.AssetItem)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SellsPrice)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Description)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FA_Master)
                </th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                @foreach (var item in Model)
                {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.FA_Details.AssetItem)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SellsPrice)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FA_Master.AssetCode)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.FA_SellId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.FA_SellId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.FA_SellId">Delete</a>
                </td>
            </tr>
                }}
            </tbody>
    </table>
