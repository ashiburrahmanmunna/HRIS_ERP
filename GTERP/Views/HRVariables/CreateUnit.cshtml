            $.ajax({

                url: '@Url.Action("DeleteUnit", "HRVariables")',
                data: JSON.stringify({ id: unitId }), //use id here
                //data: JSON.stringify(salesmain),
                type: 'POST',
                contentType: 'application/json;',
