@model IEnumerable<GTERP.ViewModels.quizAnsVms>

@{

    Layout = "~/Views/Shared/_dashlayout.cshtml";
}









    <div class="col-sm-12 col-xl-12 m-2">
        <div class="bg-light rounded h-100 p-4">

<p>
    <a asp-action="QuizSet" class="btn btn-outline-primary">Create New</a>
</p>

<div class="table-responsive">
    <table class="table table-bordered" style="width:20%" id="example">
                    <thead>
                        <tr>

                            <th>
                                ModuleName
                            </th>
                           <th>
                                Prepared by

                            </th>
                            <th>
                                Total Quiz

                            </th>
                            <th>
                                passMark

                            </th>
                            <th>
                                Timer

                            </th>
                            <th>
                                Action

                            </th>

                        </tr>
                    </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>

                                <td>
                                    @Html.DisplayFor(modelItem => item.ModuleName)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.preparedBy)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.totalques)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.passMark)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.timer)
                                </td>
                                <td>

                                    <a asp-action="QuizExamPage" asp-route-id="@item.ModuleId" class="btn btn-outline-info m-2 btn-sm">Quiz</a>
                                </td>

                </tr>
            }
        </tbody>
    </table>

</div>
</div>
</div>




