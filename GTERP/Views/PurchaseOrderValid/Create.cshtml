                                                                    <td>
                                                                        @Html.DisplayTextFor(i => itemSupplier.SupplierId)
                                                                    </td>

                                                                    <td class="addsubsection">

                                                                        @*<i class="fa fa-plus">*@
                                                                        @Html.DisplayTextFor(i => itemSupplier.vSupplier.SupplierName)
                                                                        @*</i>*@
