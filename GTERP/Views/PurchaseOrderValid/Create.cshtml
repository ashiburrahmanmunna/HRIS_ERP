                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary">Save changes</button>
                </div>
            </div>
        </div>
    </div>


}


@section Scripts
    {
    <script>



        var col = null;
        var row = null;
        var tblSuppliersearch;
        var rownovouchersub;
        var itemname;
        var antiForgeryToken;

        tblSuppliersearch = $('#tblSuppliersearch').DataTable({
            "aoColumns": [
                { "sClass": "Supplierid", "visible": true },
                { "sClass": "Suppliername", "visible": true }
            ],
            "paging": true,
            // "ordering": true,
            "searching": true,
            "info": false,
            "iDisplayLength": 5,
            "lengthMenu": [[5, 10, 25, 50, -1], [5, 10, 25, 50, "All"]],
            "oLanguage": { "sZeroRecords": "", "sEmptyTable": "" },
            dom: '<"float-left">rt<"row"<"col-sm-4"l><"col-sm-4"><"col-sm-4"p>>'
        });

        function ShowAllSubSection() {
            var cbs = document.getElementsByClassName("clickchk");
            for (var i = 0; i < cbs.length; i++) {
                cbs[i].checked = false;
            }
            //tblvoucherSupplier.columns(2).search("").draw();
            //$('#tblvoucherSupplier tbody tr').each(function () {

            //    $(this).show();

            //});
            tblvoucherSupplier.columns(2).search("").draw();


        }



        var accid;
        var tblvoucherSupplier = $('#tblvoucherSupplier').DataTable({
            "aoColumns": [
                { "sClass": "POValidSubId", "visible": false },
                { "sClass": "POValidSubSupplierId", "visible": false },
                { "sClass": "ssProductId", "visible": false },
                { "sClass": "ssRowno", "visible": false },
                { "sClass": "ssproductname", "visible": true },
                { "sClass": "ssSupplierId", "visible": false },
                { "sClass": "addsubsection", "visible": true },
                { "sClass": "ssPOValidQty", "visible": true },
                null
            ],
            rowCallback: function (row) {
                $(row).addClass('txtVoucherSupplier');
            },
            "bLengthChange": false,
            "bFilter": false,
            "bSort": false,
            "bInfo": false,
            "paging": false,
            "searching": true,
            dom: '<"float-left">rt<"row"<"col-sm-4"l><"col-sm-4"><"col-sm-4"p>>'
        });


        tblvoucherSupplier = $('#tblvoucherSupplier').DataTable();
        tblvoucherSupplier.MakeCellsEditable({
            "onUpdate": myCallbackFunction,
            "inputCss": 'my-input-class',
            "columns": [7],
            "inputTypes": [
                {
                    "column": 7,
                    "type": "text",
                    "options": null
