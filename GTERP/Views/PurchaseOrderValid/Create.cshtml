                                                    <td>
                                                        @item.vProduct.UnitId
                                                    </td>
                                                    <td>
                                                        @item.SLNo
                                                    </td>
                                                    <td>
                                                        @item.vProduct.ProductName
                                                    </td>
                                                    <td>
                                                        @item.vProduct.vProductUnit.UnitName
