                                                            <tr>
                                                                <td>@DWBCount</td>
                                                                <td>@item.UserName</td>
                                                                <td>@item.Caption</td>
                                                                <td>@item.WorkDate</td>
                                                            </tr>
                                                            DWBCount++;
                                                        }
