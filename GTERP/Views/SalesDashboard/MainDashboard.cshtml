                                                        <tr>
                                                            <th> SL NO</th>
                                                            <th> User Name</th>
                                                            <th> Caption</th>
                                                            <th> Work Date</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        @{ var DWBCount = 1;}
