                                            <i class="material-icons">info_outline</i>
                                            <a id="btnbbci" href="../DistrictWiseBookings" target="_blank">@ViewBag.CountDistrictWiseBooking.ReportLinkCaption</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col">
                                @{
                                    var l = (List<GTERP.Controllers.SalesDashboardController.Dashboard1>)ViewBag.BookingCount;
                                    var totalFirstbooking = l.FirstOrDefault();
                                    var ramainingLastbooking = l.LastOrDefault();
                                    <div class="card card-stats">
                                        <div class="card-header card-header-success card-header-icon">
                                            <div class="card-icon">
                                                <i class="material-icons">local_offer</i>
                                            </div>
                                            <p class="card-category">@ramainingLastbooking.Caption</p>
                                            <h3 class="card-title">@ramainingLastbooking.Count @*<small>GB</small>*@</h3>
                                        </div>

