                                            <div class="stats col-md-12 col-12">
                                                <i class="material-icons">local_offer</i>
                                                <a id="btnei" href="../DeliveryOrders" target="_blank"> @D.FirstOrDefault().ReportLinkCaption</a>
                                            </div>
